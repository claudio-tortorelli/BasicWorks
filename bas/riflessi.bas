SCREEN 9
CLS
COLOR 12
CIRCLE (100, 100), 50
COLOR 14
CIRCLE (250, 100), 50
COLOR 2
CIRCLE (400, 100), 50
FOR x = 1 TO 40000: NEXT x
PAINT (100, 100), 12
FOR x = 1 TO 40000: NEXT x
PAINT (250, 100), 14
20 RANDOMIZE TIMER
bul = INT(80000 * RND)
IF bul < 15000 THEN GOTO 20
FOR x = 1 TO bul: NEXT x
PAINT (400, 100), 2
TIME$ = "00:00:00"
TIMER ON
a$ = INPUT$(1)
IF TIMER = 0 THEN PRINT "stronzo"
IF TIMER > 0 THEN GOTO 500
FOR c = 1 TO 50000: NEXT c
500 CLS
PRINT TIMER
IF TIMER * 10 = .1 THEN PRINT "punteggio=100": END
IF TIMER * 10 = .2 THEN PRINT "punteggio=90": END
IF TIMER * 10 = .3 THEN PRINT "punteggio=80": END
IF TIMER * 10 = .4 THEN PRINT "punteggio=70": END
IF TIMER * 10 = .5 THEN PRINT "punteggio=60": END
IF TIMER * 10 = .6 THEN PRINT "punteggio=50": END
IF (TIMER * 10) = .7 THEN PRINT "punteggio=40": END
IF (TIMER * 10) = .8 THEN PRINT "punteggio=30": END
IF (TIMER * 10) > .9 THEN PRINT "punteggio=20": END

