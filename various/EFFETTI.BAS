CLS
SCREEN 9
FOR x = 1 TO 1000
RANDOMIZE TIMER
a = INT(3 * RND)
IF a = 1 THEN 110
IF a = 2 THEN 120
IF a = 3 THEN 130


110
RANDOMIZE TIMER
x = INT(650 * RND)
y = INT(450 * RND)
COLOR 8
CIRCLE (x, y), 1

120
RANDOMIZE TIMER
x = INT(650 * RND)
y = INT(450 * RND)
COLOR 7
CIRCLE (x, y), 1

130
RANDOMIZE TIMER
x = INT(650 * RND)
y = INT(450 * RND)
COLOR 15
CIRCLE (x, y), 1
NEXT x

